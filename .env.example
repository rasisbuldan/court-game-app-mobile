# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OAuth Redirect (for deep linking)
EXPO_PUBLIC_AUTH_REDIRECT_URL=courtster://auth/callback

# Monitoring & Analytics (Production)

# Feature Flags (set to "true" or "false")
EXPO_PUBLIC_ENABLE_SENTRY=true
EXPO_PUBLIC_ENABLE_LOKI=true
EXPO_PUBLIC_ENABLE_POSTHOG=true

# Sentry Configuration
EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# Loki Configuration (2025 - Multi-Tenant with Service Account Token)
# Get these from: Grafana Cloud > Data Sources > Loki
EXPO_PUBLIC_LOKI_PUSH_URL=https://logs-prod-032.grafana.net/loki/api/v1/push
EXPO_PUBLIC_LOKI_TENANT_ID=your_tenant_id_here
EXPO_PUBLIC_LOKI_SERVICE_ACCOUNT_TOKEN=glsa_xxxxxxxxxxxxxxxxxxxxxxxxxxxxx_xxxxxxxx

# PostHog Configuration
EXPO_PUBLIC_POSTHOG_API_KEY=your_posthog_api_key_here

# In-App Purchases (RevenueCat)
EXPO_PUBLIC_REVENUECAT_IOS_KEY=your_revenuecat_ios_key_here
EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=your_revenuecat_android_key_here
